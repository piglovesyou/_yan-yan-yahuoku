
.ac-renderer
  position: absolute
  z-index: 10
  & > .dropdown-menu
    position: static
    display: block

.goog-tooltip
  box-shadow: 2px 2px 2px rgba(0, 0, 0, .3)
  z-index: 10

// .btn:active
.btn.goog-button-checked
.btn.goog-button-selected
  background-color: #e6e6e6;
  background-color: #d9d9d9 \9;
  background-image: none;
  -webkit-box-shadow: inset 0 2px 4px rgba(0, 0, 0, 0.15), 0 1px 2px rgba(0, 0, 0, 0.05);
  -moz-box-shadow: inset 0 2px 4px rgba(0, 0, 0, 0.15), 0 1px 2px rgba(0, 0, 0, 0.05);
  box-shadow: inset 0 2px 4px rgba(0, 0, 0, 0.15), 0 1px 2px rgba(0, 0, 0, 0.05);




// goog.ui.Dialog
.modal 
  position absolute
  margin 0 // Override Bootstrap
  // background #c1d9ff
  // border 1px solid #3a5774
  // color #000
  // padding 4px


.modal a,
.modal alink,
.modal avisited  
  color #06c
  cursor pointer


.modal-bg 
  background #666
  left 0
  position absolute
  top 0
  z-index 1000


.modal-header
  border none
  cursor move

.modal-header .close 
  float none
  line-height auto
  position absolute
  top 0
  right 10px


.modal-buttons,
.modal-content 
  padding 8px


.goog-tooltip
  z-index 2000




.goog-button:focus
  border-color: rgba(82, 168, 236, 0.8);
  outline: 0;
  box-shadow: inset 0 1px 1px rgba(0, 0, 0, 0.075), 0 0 8px rgba(82, 168, 236, 0.6);

.goog-container.btn-group:focus
  outline: 0;
  box-shadow:
    inset 0 1px 1px rgba(0, 0, 0, 0.075),
    0 0 8px rgba(82, 168, 236, 0.6)
  .btn
    border-color: rgba(82, 168, 236, 0.8);
  .btn:not(:first-child)
    border-left-color rgba(0, 0, 0, 0.15);
